<project xmlns="http://maven.apache.org/POM/4.0.0"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  	xsi:schemaLocation="http://maven.apache.org/POM/4.0.0
	http://maven.apache.org/maven-v4_0_0.xsd">
  <modelVersion>4.0.0</modelVersion>
  <groupId>com.orange</groupId>
  <artifactId>HelloWorld</artifactId>
  <packaging>war</packaging>
  <version>1.0-SNAPSHOT</version>
  <name>HelloWorld</name>
  <url>http://maven.apache.org</url>
	<scm>
	  <url>https://github.com/williwanka/HelloWorld.git</url>
	  <connection>scm:git:git://github.com/williwanka/HelloWorld.git</connection>
	  <developerConnection>scm:git:git@github.com:williwanka/HelloWorld.git</developerConnection>
	</scm>
  <dependencies>
    <dependency>
      <groupId>junit</groupId>
      <artifactId>junit</artifactId>
      <version>3.8.1</version>
      <scope>test</scope>
    </dependency>
  </dependencies>
  <build>
		 <plugins>
		<plugin>
		<groupId>org.apache.maven.plugins</groupId>
		<artifactId>maven-war-plugin</artifactId>
		<configuration>
			<webXml>src\main\WebApp\WEB-INF\web.xml</webXml>
		</configuration>
	</plugin>
	 </plugins>
		   <finalName>HelloWorld</finalName>
  </build>
</project>
